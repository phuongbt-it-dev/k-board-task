extends _layout.pug
block variables
	- var title = 'Chỉnh sửa thông tin thành viên'
	- var hasmenu = 'hasmenu'
block header
	if process.env.ENVGLOBAL === 'true'
		script
			include _header.js
	else
		include _header.pug
block content
	if process.env.ENVGLOBAL === 'true'
		script
			include __edituser.js
			include ./assets/edituser.min.js
	else
		include __edituser
		script(key=val, src='./js/edituser.js?v='+text)
block footer
	if process.env.ENVGLOBAL === 'true'
		script
			include _footer.js
	else
		include _footer.pug
