extends _layout.pug
block variables
	- var title = 'Bảng nhiệm vụ'
	- var hasmenu = 'hasmenu'
block header
	if process.env.ENVGLOBAL === 'true'
		script
			include _header.js
	else
		include _header.pug
block content
	if process.env.ENVGLOBAL === 'true'
		script
			include __index.js
			include ./assets/index.min.js
	else
		include __index
		script(key=val, src='./js/index.js?v='+text)
block footer
	if process.env.ENVGLOBAL === 'true'
		script
			include _footer.js
	else
		include _footer.pug

