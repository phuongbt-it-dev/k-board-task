extends _layout.pug
block variables
	- var title = 'Tuỳ chỉnh'
	- var hasmenu = 'hasmenu'
block header
	if process.env.ENVGLOBAL === 'true'
		script
			include _header.js
	else
		include _header.pug
block content
	if process.env.ENVGLOBAL === 'true'
		script
			include __settings.js
			include ./assets/settings.min.js
	else
		include __settings
		script(key=val, src='./js/settings.js?v='+text)
block footer
	if process.env.ENVGLOBAL === 'true'
		script
			include _footer.js
	else
		include _footer.pug
